<mvc:View
    controllerName="dma.zfichatec.controller.imprimirDetalhe"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:unified="sap.ui.unified" >
	<App>
        <pages>
            <Page
                id="idPageImprimirDetalhe01"
                showHeader="true"
                title="{i18n>titulo_imprimir_detalhe}"
                enableScrolling="true"
                navButtonPress="onNavBack"
                showNavButton="true">
                <content>
                    <core:HTML
                        id="idFrame01" />
                    <Label
                        text="{i18n>email_lbl}:"
                        textAlign="End"
                        class="sapUiSmallMarginTop sapUiSmallMarginBegin sapUiSMallMarginEnd label_h3" />
                    <Input
                        id="idInputEmail1"
                        width="400px"
                        class="sapUiSmallMarginBegin sapUiSmallMarginEnd" />
                    <Button
                        text="Enviar por e-mail"
                        press="onEnviarEmailDetalhe"
                        class="sapUiTinyMarginBegin" />
                </content>
            </Page>
        </pages>
    </App>
</mvc:View>
<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<SelectDialog
		title="Sortimento"
		class="sapUiPopupWithPadding"
		items="{/SortimSet}"
        multiSelect="true"
        rememberSelections="true"
		search="onValueHelpSortimentoSearch"
		confirm="onValueHelpSortimentoClose"
		cancel="onValueHelpSortimentoClose"
        showClearButton="true" >
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Asort}"
			description="{Name1}"
			info="" />
	</SelectDialog>
</core:FragmentDefinition>
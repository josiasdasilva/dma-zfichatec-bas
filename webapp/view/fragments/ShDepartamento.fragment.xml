<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<SelectDialog
		title="{i18n>departamento_lbl}"
		class="sapUiPopupWithPadding"
		items="{/DepartamentoSet}"
        multiSelect="true"
		search="onValueHelpDepartamentoSearch"
		confirm="onValueHelpDepartamentoClose"
		cancel="onValueHelpDepartamentoCancel">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Node3}"
			description="{Ltext}"
			info="" />
	</SelectDialog>
</core:FragmentDefinition>
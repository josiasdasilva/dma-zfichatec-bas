<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<SelectDialog
		title="{i18n>contrato_lbl}"
		class="sapUiPopupWithPadding"
		items="{/ContratoSet}"
        multiSelect="true"
		liveChange="onValueHelpContratoSearch"
		confirm="onValueHelpContratoClose"
		cancel="onValueHelpContratoCancel">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Ebeln}"
			description="{Mcod1}"
			info="{Ekgrp}" />
	</SelectDialog>
</core:FragmentDefinition>
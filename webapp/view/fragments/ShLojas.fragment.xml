<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<SelectDialog
		title="{i18n>loja_lbl}"
		class="sapUiPopupWithPadding"
		items="{/LojasSet}"
        multiSelect="true"
		liveChange="onValueHelpLojasSearch"
		confirm="onValueHelpLojasClose"
		cancel="onValueHelpLojasCancel">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Werks}"
			description="{Nome}"
			info="{UF}" />
	</SelectDialog>
</core:FragmentDefinition>
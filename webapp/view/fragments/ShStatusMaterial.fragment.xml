<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<SelectDialog
		title="{i18n>status_mat_lbl}"
		class="sapUiPopupWithPadding"
		items="{/StatusMatSet}"
        multiSelect="true"
        rememberSelections="true"
		search="onValueHelpStatusMaterialSearch"
		confirm="onValueHelpStatusMaterialClose"
		cancel="onValueHelpStatusMaterialClose"
        showClearButton="true" >
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{MMSTA}"
			description="{MTSTB}"
			info="" />
	</SelectDialog>
</core:FragmentDefinition>
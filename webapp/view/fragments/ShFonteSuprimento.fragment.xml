<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<SelectDialog
		title="{i18n>fonte_supr_lbl}"
		class="sapUiPopupWithPadding"
		items="{/FSupSet}"
        multiSelect="true"
		search="onValueHelpFonteSuprimentoSearch"
		confirm="onValueHelpFonteSuprimentoClose"
		cancel="onValueHelpFonteSuprimentoCancel" >
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Sobsl}"
			description="{Ltext}"
			info="" />
	</SelectDialog>
</core:FragmentDefinition>
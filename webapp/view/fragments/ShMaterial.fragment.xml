<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<SelectDialog
		title="{i18n>material_lbl}"
		class="sapUiPopupWithPadding"
		items="{/MaterialSet}"
        multiSelect="true"
		liveChange="onValueHelpMaterialSearch"
		confirm="onValueHelpMaterialClose"
		cancel="onValueHelpMaterialCancel">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Matnr}"
			description="{Maktx}"
			info="" />
	</SelectDialog>
</core:FragmentDefinition>
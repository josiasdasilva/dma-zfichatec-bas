<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<SelectDialog
		title="{i18n>fornecedor_lbl}"
		class="sapUiPopupWithPadding"
		items="{/FornecedorSet}"
        multiSelect="true"
		search="onValueHelpFornecedorSearch"
		confirm="onValueHelpFornecedorClose"
		cancel="onValueHelpFornecedorCancel">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Lifnr}"
			description="{Mcod1}"
			info="{Ekgrp}" />
	</SelectDialog>
</core:FragmentDefinition>
<mvc:View
    controllerName="dma.zfichatec.controller.Home"
    xmlns="sap.m"
    xmlns:control="sap.gantt.control"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:unified="sap.ui.unified" >
	<App id="homeId">
        <NavContainer id="idWizardNavContainer1">
            <pages>
                <Page
                    id="idWizardContentPage1"
                    showHeader="false"
                    title="{i18n>appTitle}"
                    enableScrolling="false">
<!--
				<FlexBox width="100%" direction="Column" fitContainer="true">
					<items>
						<FlexBox width="100%" direction="Column" fitContainer="true">
							<items>
								<FlexBox width="100%" justifyContent="Center" height="auto" fitContainer="true" alignContent="Center">
									<items>
										<Image width="auto" height="470px" id="img_epa" src="img/background_cockpit.png" densityAware="false"/>
									</items>
								</FlexBox>
							</items>
						</FlexBox>
					</items>
				</FlexBox>
				<content/>
-->
                    <content>

                        <Wizard
                            id="idWizard01"
                            finishButtonText="{i18n>pesquisar_lbl}"
                            complete="handleWizardCompleted">

                            <WizardStep id="idWizardStep1"
                                        title="{i18n>tela_inicial_tit}"
                                        validated="true">

                                <HBox
                                    alignItems="Start"
                                    justifyContent="Start"
                                    width="50%">
                                    <Label
                                        text="{i18n>comprador_lbl}:"
                                        width="76px"
                                        class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!--
                                    <Input
                                        id="idInputCompradorCod1"
                                        maxLength="3"
                                        value="{modelScreenParams>/screen1/idInputCompradorCod1}"
                                        valueLiveUpdate="true"
                                        width="85px"
                                        showValueHelp="true"
                                        valueHelpRequest="onValueHelpComprador"
                                        change="onChangeInputField" />
                                    <Input
                                        id="idInputCompradorDescr1"
                                        value="{modelScreenParams>/screen1/idInputCompradorDescr1}"
                                        enabled="false"
                                        class="sapUiSmallMarginBegin" />
-->
                                    <MultiInput
                                        width="350px"
                                        id="idMultiInputCompradorCod1"
                                        tokens="{
                                            path: 'modelScreenParams>/screen1/idMultiInputCompradorCod1'
                                        }"
                                        tokenChange="onTokenChangeMultiInput"
                                        valueHelpRequest="onValueHelpComprador" >
                                        <tokens>
                                            <Token
                                                key="{modelScreenParams>Ekgrp}"
                                                text="{modelScreenParams>Ekgrp}" />
                                        </tokens>
                                    </MultiInput>
                                </HBox>
                                    
                                <HBox
                                    alignItems="Start"
                                    justifyContent="Start"
                                    width="50%">
                                    <Label
                                        text="{i18n>fornecedor_lbl}:"
                                        width="76px"
                                        class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!-- 
                                    <Input
                                        id="idInputFornecedorCod1"
                                        maxLength="10"
                                        value="{modelScreenParams>/screen1/idInputFornecedorCod1}"
                                        width="140px"
                                        showValueHelp="true"
                                        valueHelpRequest="onValueHelpFornecedor"
                                        change="onChangeInputField" />
                                    <Input
                                        id="idInputFornecedorDescr1"
                                        value="{modelScreenParams>/screen1/idInputFornecedorDescr1}"
                                        enabled="false"
                                        class="sapUiSmallMarginBegin" />
 -->
                                    <MultiInput
                                        width="350px"
                                        id="idMultiInputFornecedorCod1"
                                        tokens="{
                                            path: 'modelScreenParams>/screen1/idMultiInputFornecedorCod1'
                                        }"
                                        tokenChange="onTokenChangeMultiInput"
                                        valueHelpRequest="onValueHelpFornecedor" >
                                        <tokens>
                                            <Token key="{modelScreenParams>Lifnr}" text="{modelScreenParams>Lifnr}" />
                                        </tokens>
                                    </MultiInput>
                                </HBox>
                                    
                                <HBox
                                    alignItems="Start"
                                    justifyContent="Start"
                                    width="50%">
                                    <Label
                                        text="{i18n>contrato_lbl}:"
                                        width="76px"
                                        class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!-- 
                                    <Input
                                        id="idInputContrato1"
                                        maxLength="10"
                                        value="{modelScreenParams>/screen1/idInputContrato1}"
                                        width="140px"
                                        showValueHelp="true"
                                        valueHelpRequest="onValueHelpContrato" />
 -->
                                    <MultiInput
                                        width="350px"
                                        id="idMultiInputContrato1"
                                        tokens="{
                                            path: 'modelScreenParams>/screen1/idMultiInputContrato1'
                                        }"
                                        tokenChange="onTokenChangeMultiInput"
                                        valueHelpRequest="onValueHelpContrato" >
                                        <tokens>
                                            <Token key="{modelScreenParams>Ebeln}" text="{modelScreenParams>Ebeln}" />
                                        </tokens>
                                    </MultiInput>
                                </HBox>

                            </WizardStep>

                            <WizardStep id="idWizardStep2"
                                        title="{i18n>vis_relat_tit}"
                                        validated="true">
                                <HBox
                                    alignItems="Start"
                                    justifyContent="Start"
                                    width="50%">
                                    <RadioButton
                                        id="idRadioButtonVisRelatLoja"
                                        groupName="GroupVisRelat"
                                        text="{i18n>loja_lbl}"
                                        class="sapUiMediumMarginEnd"
                                        selected="{= ${modelScreenParams>/screen1/GroupVisRelat} === '1' }"
                                        select="onRadioButtonSelect" />
                                    <RadioButton
                                        id="idRadioButtonVisRelatGrpPrecos"
                                        groupName="GroupVisRelat"
                                        text="{i18n>grp_precos_lbl}"
                                        class="sapUiMediumMarginEnd"
                                        selected="{= ${modelScreenParams>/screen1/GroupVisRelat} === '2' }"
                                        select="onRadioButtonSelect" />
                                    <RadioButton
                                        id="idRadioButtonVisRelatFonteSupr"
                                        groupName="GroupVisRelat"
                                        text="{i18n>fonte_supr_lbl}"
                                        class="sapUiMediumMarginEnd"
                                        selected="{= ${modelScreenParams>/screen1/GroupVisRelat} === '3' }"
                                        select="onRadioButtonSelect" />
                                </HBox>

                                <HBox
                                    alignItems="Start"
                                    justifyContent="Start"
                                    width="50%">
                                    <RadioButton
                                        id="idRadioButtonVisRelatUf"
                                        groupName="GroupVisRelat"
                                        text="{i18n>uf_lbl}"
                                        class="sapUiMediumMarginEnd"
                                        selected="{= ${modelScreenParams>/screen1/GroupVisRelat} === '4' }"
                                        select="onRadioButtonSelect" />
                                    <RadioButton
                                        id="idRadioButtonVisRelatSortim"
                                        groupName="GroupVisRelat"
                                        text="{i18n>sortim_lbl}"
                                        selected="{= ${modelScreenParams>/screen1/GroupVisRelat} === '5' }"
                                        select="onRadioButtonSelect" />
                                </HBox>
                            </WizardStep>

                            <WizardStep id="idWizardStep3"
                                        title="{i18n>filtros_tit}"
                                        validated="true">
                                <HBox
                                    alignItems="Start"
                                    justifyContent="Start"
                                    width="50%"
                                    class="sapUiLargeMarginEnd" >
                                    
                                    <VBox>
                                        <CheckBox
                                            id="idCheckBoxSomenteMatXdock1"
                                            text="{i18n>somente_mat_xdock_lbl}"
                                            selected="{= ${modelScreenParams>/screen1/idCheckBoxSomenteMatXdock1} === true }"
                                            select="onCheckBoxSelect" />
                                        
                                        <HBox
                                            class="sapUiSmallMarginTop" >
                                            <Label
                                                text="{i18n>fonte_supr_lbl}:"
                                                class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!-- 
                                            <Input
                                                id="idInputFonteSuprimento1"
                                                maxLength="10"
                                                value="{modelScreenParams>/screen1/idInputFonteSuprimento1}"
                                                width="140px"
                                                showValueHelp="true" />
 -->
                                            <MultiInput
                                                width="200px"
                                                id="idMultiInputFonteSuprimento1"
                                                tokens="{
                                                    path: 'modelScreenParams>/screen1/idMultiInputFonteSuprimento1'
                                                }"
                                                valueHelpRequest="onValueHelpFonteSuprimento">
                                                <tokens>
                                                    <Token key="{modelScreenParams>}" text="{modelScreenParams>}" />
                                                </tokens>
                                            </MultiInput>
                                        </HBox>

                                        <HBox
                                            class="sapUiTinyMarginTop" >
                                            <Label
                                                text="{i18n>status_mat_lbl}: ="
                                                class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!-- 
                                            <Input
                                                id="idInputStatusMat1"
                                                maxLength="3"
                                                value="{modelScreenParams>/screen1/idInputStatusMat1}"
                                                width="85px"
                                                showValueHelp="true" />
 -->
                                            <MultiInput
                                                width="205px"
                                                id="idMultiInputStatusMat1"
                                                tokens="{
                                                    path: 'modelScreenParams>/screen1/idMultiInputStatusMat1'
                                                }"
                                                valueHelpRequest="onValueHelpStatusMat">
                                                <tokens>
                                                    <Token key="{modelScreenParams>}" text="{modelScreenParams>}" />
                                                </tokens>
                                            </MultiInput>
                                        </HBox>

                                        <HBox
                                            class="sapUiTinyMarginTop" >
                                            <Label
                                                text="{i18n>departamento_lbl}:"
                                                class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!-- 
                                            <Input
                                                id="idInputDepartamento1"
                                                maxLength="5"
                                                value="{modelScreenParams>/screen1/idInputDepartamento1}"
                                                width="100px"
                                                showValueHelp="true" />
 -->
                                            <MultiInput
                                                width="242px"
                                                id="idMultiInputDepartamento1"
                                                tokens="{
                                                    path: 'modelScreenParams>/screen1/idMultiInputDepartamento1'
                                                }"
                                                valueHelpRequest="onValueHelpDepartamento">
                                                <tokens>
                                                    <Token key="{modelScreenParams>Node3}" text="{modelScreenParams>Node3}" />
                                                </tokens>
                                            </MultiInput>
                                        </HBox>

                                        <HBox
                                            class="sapUiTinyMarginTop" >
                                            <Label
                                                text="{i18n>no_hierarquia_lbl}:"
                                                class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!-- 
                                            <Input
                                                id="idInputNoHierarquia1"
                                                maxLength="12"
                                                value="{modelScreenParams>/screen1/idInputNoHierarquia1}"
                                                width="160px"
                                                showValueHelp="true" />
 -->
                                            <MultiInput
                                                width="225px"
                                                id="idMultiInputNoHierarquia1"
                                                tokens="{
                                                    path: 'modelScreenParams>/screen1/idMultiInputNoHierarquia1'
                                                }"
                                                valueHelpRequest="onValueHelpHierarquia">
                                                <tokens>
                                                    <Token key="{modelScreenParams>Node6}" text="{modelScreenParams>Node6}" />
                                                </tokens>
                                            </MultiInput>
                                        </HBox>

                                    </VBox>

                                    <VBox
                                        class="sapUiLargeMarginBegin" >
                                        <Label text="{i18n>bandeira_lbl}:" />
                                        <HBox>
                                            <CheckBox
                                                id="idCheckBoxAtacado1"
                                                text="{i18n>atacado_lbl}"
                                                selected="{= ${modelScreenParams>/screen1/idCheckBoxAtacado1} === true }"
                                                select="onCheckBoxSelect" />
                                            <CheckBox
                                                id="idCheckBoxVarejo1"
                                                text="{i18n>varejo_lbl}"
                                                selected="{= ${modelScreenParams>/screen1/idCheckBoxVarejo1} === true }"
                                                select="onCheckBoxSelect" />
                                        </HBox>

                                        <Label
                                            text="{i18n>filtros_visoes_lbl}:"
                                            class="sapUiSmallMarginTop" />
                                        <HBox
                                            class="sapUiTinyMarginTop" >
                                            <Label
                                                text="{i18n>uf_lbl}:"
                                                width="50px"
                                                class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!-- 
                                            <Input
                                                id="idInputUf1"
                                                maxLength="2"
                                                value="{modelScreenParams>/screen1/idInputUf1}"
                                                width="85px"
                                                showValueHelp="true"
                                                valueHelpRequest="onValueHelpUf" />
 -->
                                            <MultiInput
                                                width="200px"
                                                id="idMultiInputUf1"
                                                tokens="{
                                                    path: 'modelScreenParams>/screen1/idMultiInputUf1'
                                                }"
                                                tokenChange="onTokenChangeMultiInput"
                                                valueHelpRequest="onValueHelpUf" >
                                                <tokens>
                                                    <Token key="{modelScreenParams>Bland}" text="{modelScreenParams>Bland}" />
                                                </tokens>
                                            </MultiInput>

                                            <Label
                                                text="{i18n>grp_precos_lbl}:"
                                                width="110px"
                                                class="sapUiSmallMarginTop sapUiSmallMarginBegin" />
<!-- 
                                            <Input
                                                id="idInputGrpPrecos1"
                                                maxLength="3"
                                                value="{modelScreenParams>/screen1/idInputGrpPrecos1}"
                                                width="85px"
                                                showValueHelp="true"
                                                valueHelpRequest="onValueHelpGrpPrecos"
                                                class="sapUiTinyMarginBegin" />
 -->
                                            <MultiInput
                                                width="200px"
                                                id="idMultiInputGrpPrecos1"
                                                tokens="{
                                                    path: 'modelScreenParams>/screen1/idMultiInputGrpPrecos1'
                                                }"
                                                tokenChange="onTokenChangeMultiInput"
                                                valueHelpRequest="onValueHelpGrpPrecos" >
                                                <tokens>
                                                    <Token key="{modelScreenParams>Bandeira}" text="{modelScreenParams>Bandeira}" />
                                                </tokens>
                                            </MultiInput>
                                        </HBox>

                                        <HBox
                                            class="sapUiTinyMarginTop" >
                                            <Label
                                                text="{i18n>loja_lbl}:"
                                                width="50px"
                                                class="sapUiSmallMarginTop sapUiSmallMarginEnd" />
<!-- 
                                            <Input
                                                id="idInputLoja1"
                                                maxLength="4"
                                                value="{modelScreenParams>/screen1/idInputLoja1}"
                                                width="85px"
                                                showValueHelp="true"
                                                valueHelpRequest="onValueHelpLoja" />
 -->
                                            <MultiInput
                                                width="200px"
                                                id="idMultiInputLoja1"
                                                tokens="{
                                                    path: 'modelScreenParams>/screen1/idMultiInputLoja1'
                                                }"
                                                tokenChange="onTokenChangeMultiInput"
                                                valueHelpRequest="onValueHelpLoja" >
                                                <tokens>
                                                    <Token key="{modelScreenParams>Werks}" text="{modelScreenParams>Werks}" />
                                                </tokens>
                                            </MultiInput>

                                            <Label
                                                text="{i18n>sortim_lbl}:"
                                                width="110px"
                                                class="sapUiSmallMarginTop sapUiSmallMarginBegin" />
<!-- 
                                            <Input
                                                id="idInputSortim1"
                                                maxLength="8"
                                                value="{modelScreenParams>/screen1/idInputSortim1}"
                                                width="140px"
                                                showValueHelp="true"
                                                class="sapUiTinyMarginBegin" />
 -->
                                            <MultiInput
                                                width="200px"
                                                id="idMultiInputSortim1"
                                                tokens="{
                                                    path: 'modelScreenParams>/screen1/idMultiInputSortim1'
                                                }"
                                                valueHelpRequest="onValueHelpSortim">
                                                <tokens>
                                                    <Token key="{modelScreenParams>}" text="{modelScreenParams>}" />
                                                </tokens>
                                            </MultiInput>
                                        </HBox>

                                        <HBox>
                                            <CheckBox
                                                id="idCheckBoxTotUf1"
                                                text="{i18n>tot_uf_lbl}"
                                                selected="{= ${modelScreenParams>/screen1/idCheckBoxTotUf1} === true }"
                                                select="onCheckBoxSelect" />
                                            <CheckBox
                                                id="idCheckBoxTotGrp1"
                                                text="{i18n>tot_grp_lbl}"
                                                selected="{= ${modelScreenParams>/screen1/idCheckBoxTotGrp1} === true }"
                                                select="onCheckBoxSelect" />
                                        </HBox>
                                    </VBox>

                                </HBox>
                            </WizardStep>
                        </Wizard>
                    </content>

                    <footer>
                        <OverflowToolbar>
                            <ToolbarSpacer/>
                            <Button text="{i18n>reiniciar_pesquisa_lbl}" press="handleWizardResetFilters"/>
                        </OverflowToolbar>
                    </footer>

                </Page>
            </pages>
        </NavContainer>
	</App>
</mvc:View>